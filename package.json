{
  "name": "blog",
  "version": "0.0.1",
  "scripts": {
    "dev": "svelte-kit dev --host 0.0.0.0",
    "build": "svelte-kit build",
    "package": "svelte-kit package",
    "preview": "svelte-kit preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "test": "vitest --run",
    "test-files": "vitest --run related",
    "test:watch": "vitest",
    "lint": "npm run lint:js-files -- . && npm run lint:css-files -- \"src/**/*.{css,svelte,svg}\"",
    "lint:js-files": "prettier --ignore-path .gitignore --check --plugin-search-dir=.",
    "lint:css-files": "stylelint --config .stylelintrc.json",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^1.0.0-next.33",
    "@sveltejs/adapter-static": "^1.0.0-next.29",
    "@sveltejs/kit": "^1.0.0-next.299",
    "@testing-library/svelte": "^3.1.0",
    "@types/d3": "^7.1.0",
    "@types/js-yaml": "^4.0.5",
    "@types/seedrandom": "^3.0.2",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "autoprefixer": "^10.4.2",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-svelte3": "^3.4.1",
    "js-yaml": "^4.1.0",
    "jsdom": "^19.0.0",
    "mdsvex": "^0.10.5",
    "postcss": "^8.4.5",
    "postcss-html": "^1.3.0",
    "postcss-load-config": "^3.1.1",
    "postcss-nested": "^5.0.6",
    "prettier": "^2.6.0",
    "prettier-plugin-svelte": "^2.5.0",
    "prism-themes": "^1.9.0",
    "rehype-autolink-headings": "^6.1.1",
    "rehype-slug": "^5.0.1",
    "remark-abbr": "^1.4.1",
    "stylelint": "^14.6.0",
    "stylelint-config-css-modules": "^4.1.0",
    "stylelint-config-html": "^1.0.0",
    "stylelint-config-idiomatic-order": "^8.1.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-order": "^5.0.0",
    "svelte": "^3.46.4",
    "svelte-check": "^2.4.6",
    "svelte-preprocess": "^4.10.1",
    "ts-essentials": "^9.1.2",
    "tslib": "^2.3.1",
    "typescript": "^4.6.2",
    "vitest": "^0.8.0",
    "vitest-svelte-kit": "^0.0.6"
  },
  "type": "module"
}
